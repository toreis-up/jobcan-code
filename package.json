{
  "name": "jobcan-code",
  "displayName": "jobcan-code",
  "description": "Touch jobcan in vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jobcan-code.jobcan-login",
        "title": "Jobcan: Login"
      },
      {
        "command": "jobcan-code.jobcan-set-username",
        "title": "Jobcan: Set username"
      },
      {
        "command": "jobcan-code.jobcan-set-password",
        "title": "Jobcan: Set password"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/got": "^9.6.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/tough-cookie": "^4.0.5",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "got": "^14.2.0",
    "http-cookie-agent": "^6.0.1",
    "node-html-parser": "^6.1.12",
    "tough-cookie": "^4.1.3",
    "undici": "^6.6.2"
  }
}
